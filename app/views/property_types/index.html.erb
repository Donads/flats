<div>
  <p><%= link_to 'Cadastrar', new_property_type_path %></p>
</div>

<% if @property_types.empty? %>
  <div>
    <p>Nenhum tipo cadastrado</p>
  </div>
<% end %>

<div>
  <p>
    <% @property_types.each do |prop| %>
      <p>
        <%= prop.name %>
      </p>
    <% end %>
  </p>
</div>

<div>
  <p><%= link_to 'Voltar', root_path %></p>
</div>
