<div>
  <p><%= link_to 'Cadastrar', new_property_type_path %></p>
</div>

<% if @property_types.empty? %>
  <div>
    <p>Nenhum tipo cadastrado</p>
  </div>
<% end %>

<div>
  <p>
    <% @property_types.each do |prop| %>
      <p>
        <%= link_to prop.name, property_type_path(prop) %>
        <%= link_to 'Editar', edit_property_type_path(prop) %>
      </p>
    <% end %>
  </p>
</div>

<div>
  <p><%= link_to t('links.go_back'), root_path %></p>
</div>
